{
    "name": "juggernaut/oe-module-8x8-jitsi",
    "description": "OpenEMR Custom Module 8x8 Project",
    "type": "openemr-module",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Sherwin Gaddis",
            "email": "sherwingaddis@gmail.com",
            "role": "Developer"
        }
    ],
    "keywords": ["openemr", "openemr-module"],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {"Juggernaut\\Modules\\Jitsi8x8Telehealth\\": "src/"}
    },
    "require": {
        "openemr/oe-module-installer-plugin": "^0.1.0",
        "php": ">= 7.4",
        "symfony/event-dispatcher": "^4.4.0",
        "nyholm/psr7": "^1.4",
        "web-token/jwt-key-mgmt": "^2.2",
        "web-token/jwt-signature": "^2.2",
        "web-token/jwt-signature-algorithm-rsa": "^2.2",
        "firebase/php-jwt": "^5.2",
        "web-token/jwt-core": "^2.2",
        "web-token/jwt-signature-algorithm-hmac": "*"
    },
    "conflict": {
        "openemr/openemr": "<7.0.0"
    },
    "config": {
        "allow-plugins": {
            "openemr/oe-module-installer-plugin": true
        }
    }
